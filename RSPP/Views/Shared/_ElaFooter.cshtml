<script>
    function SearchApp() {

        var content = $("#txtSearchContent").val();

        if (content === null || content === "") {
            return false;
        }
        else {
            var location = window.location.origin + "/Applications/Search/" + content;
            window.location.href = location;
        }
    }
</script>